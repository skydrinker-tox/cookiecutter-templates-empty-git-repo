    "project_name": "Project name",
    "project_slug": "Root directory for new repo",
    "init_git": "Initialize a git repository ?",
    "git_email": "Git email (only for git init)",
    "git_username": "Git username (only for git init)",
    "push_git": "Push initial commit to an empty git repository ?",
    "git_repo_url": "Git repo url (only for git init)",
    "git_main_branch": "Git main branch (only for git init)"
  },
  "_new_lines": "\n",
